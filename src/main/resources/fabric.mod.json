{
  "schemaVersion": 1,
  "id": "woodenwalls",
  "version": "${version}",
  "name": "WoodenWalls",
  "description": "An exceedingly simple mod that adds wooden walls to minecraft",
  "authors": [
    "PhoenixOrigin"
  ],
  "contact": {
    "repo": "https://github.com/amogh/WoodenWalls"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/woodenwalls/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.phoenix.woodenwalls.client.WoodenWallsClient"
    ],
    "main": [
      "net.phoenix.woodenwalls.WoodenWalls"
    ],
    "fabric-datagen": [
      "net.phoenix.woodenwalls.datagen.ModDatagen"
    ]
  },
  "mixins": [
    "woodenwalls.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}"
  }
}
